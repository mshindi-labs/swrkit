{
  "name": "@mshindi-labs/swrkit",
  "version": "1.0.0",
  "description": "A powerful, apisauce-inspired wrapper around SWR for standardized API data fetching in React/Next.js applications",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "swr",
    "react",
    "hooks",
    "data-fetching",
    "api",
    "client",
    "typescript",
    "refetch",
    "http",
    "rest"
  ],
  "author": "mshindi-labs",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "swr": "^2.0.0"
  },
  "dependencies": {
    "@mshindi-labs/refetch": "^2.0.7"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "swr": "^2.3.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
